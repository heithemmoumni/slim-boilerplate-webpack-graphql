{
    "name": "heithemmoumni/slim-skeleton-webpack",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7",
        "webpack"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Heithem Moumni",
            "email": "moumniheithem@gmail.com",
            "homepage": "https://www.hmoumni.com/"
        }
    ],
    "require": {
        "ext-json": "*",
        "monolog/monolog": "^2.0",
        "php": "^7.2",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
